{
	"name": "@whatsnew/parsers",
	"version": "0.4.0",
	"description": "Multi-format changelog parsers for Keep-a-Changelog, Conventional Commits, Changesets, and more",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "vitest --run",
		"test:coverage": "vitest --run --coverage",
		"typecheck": "tsc --noEmit"
	},
	"license": "MIT",
	"author": "Gustavo Valverde",
	"repository": {
		"type": "git",
		"url": "https://github.com/gustavovalverde/whatsnew",
		"directory": "packages/parsers"
	},
	"homepage": "https://github.com/gustavovalverde/whatsnew#readme",
	"bugs": {
		"url": "https://github.com/gustavovalverde/whatsnew/issues"
	},
	"keywords": [
		"changelog",
		"releases",
		"whatsnew",
		"wnf",
		"parser",
		"keep-a-changelog",
		"conventional-commits",
		"changesets"
	],
	"dependencies": {
		"@whatsnew/types": "workspace:*",
		"@whatsnew/utils": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "^24.10.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.15",
		"vite": "^7.2.7"
	},
	"engines": {
		"node": ">=24"
	}
}
